pipeline
{
  agent any
  stages
  {
    stage("CHECKOUT")
    {
      steps
      {
        git 'https://github.com/Sushil-Ohol/Nitor-Test.git'
        echo 'THIS IS CHECKOUT STAGE'
      }
    }
    stage("BUILD")
    {
      steps
      {
        touch file: 'HASH.txt', timestamp: 0
        
      }
    }
    stage("BUILD")
    {
      steps
      {
        
      }
    }
  }
}
